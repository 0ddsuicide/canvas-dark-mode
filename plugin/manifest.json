{
  "name": "Canvas Dark Mode",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "Dark Mode for Canvas.",
  "homepage_url": "https://github.com/DeGrandis/canvas-dark-mode",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "19": "icons/icon19.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "test"
  },
  "default_locale": "en",
  "permissions": [
      "https://*.instructure.com/*",
      "https://canvas.vt.edu/*",
      "https://canvas.oregonstate.edu/*",
      "https://canvas.uw.edu/*",
      "https://canvas.upenn.edu/*",
      "https://canvas.du.edu/*",
      "https://canvas.morainevalley.edu/*",
      "https://bcourses.berkeley.edu/*",
      "https://canvas.uchicago.edu/*",
      "https://canvas.tufts.edu/*",
      "https://webcourses.ucf.edu/*",
      "https://canvas.harvard.edu/*",
      "https://canvas.auckland.ac.nz/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.instructure.com/*",
        "https://canvas.vt.edu/*",
        "https://canvas.oregonstate.edu/*",
        "https://canvas.uw.edu/*",
        "https://canvas.upenn.edu/*",
        "https://canvas.du.edu/*",
        "https://canvas.morainevalley.edu/*",
        "https://bcourses.berkeley.edu/*",
        "https://canvas.uchicago.edu/*",
        "https://canvas.tufts.edu/*",
        "https://webcourses.ucf.edu/*",
        "https://canvas.harvard.edu/*",
        "https://canvas.auckland.ac.nz/*"
      ],
      "css":[
          "css/styles.css"
      ]
    }
  ]
}
