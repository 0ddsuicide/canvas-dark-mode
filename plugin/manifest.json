{
  "name": "Canvas Dark Mode",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Dark Mode for Canvas.",
  "homepage_url": "https://github.com/DeGrandis/canvas-dark-mode",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
      "https://canvas.vt.edu/*",
      "https://*.instructure.com/*",
      "https://canvas.oregonstate.edu/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://canvas.vt.edu/*",
        "https://*.instructure.com/*",
        "https://canvas.oregonstate.edu/*"
      ],
      "css":[
          "css/styles.css"
      ]
    }
  ]
}
